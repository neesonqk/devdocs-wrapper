{
  "name": "DevDocs",
  "version": "1.0.0",
  "description": "A minimal wrapper for DevDocs",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "package-osx": "node_modules/.bin/electron-packager . DevDocs --platform=darwin --arch=x64 --icon=icon.icns",
